{
  "name": "react-app-dawn",
  "version": "1.0.0",
  "repository": {
    "url": "git@github.com:codylindley/react-app-dawn.git",
    "type": "git"
  },
  "author": "Cody Lindley <contact@codylindley.com>",
  "license": "MIT",
  "scripts": {
    "0///////////////////////////////////////////        start:dev          //////////////////////////////////////": "",
    "Start webpack dev server, uses the webpack.config.babel.js": "",
    "-d is a shortcut for --debug --devtool eval-cheap-module-source-map --output-pathinfo": "",
    "--content-base is which directory is being servered": "",
    "--open will open the code found in src directory at localhost:8080": "",
    "start:dev": "webpack-dev-server -d --content-base src --open --history-api-fallback",
    "1//////////////////////////////////////////        build         ////////////////////////////////////////////": "",
    "Run js and CSS linters": "",
    "rimraf build directory then (i.e. rimraf is a deep deletion module for node):": "",
    "Build production code using webpack cli, uses the webpack.config.babel.js file": "",
    "usae --env=production, which passes the env value to the webpack config function": "",
    "build": "npm run lint:js && npm run lint:css && rimraf build && webpack --env=production",
    "2//////////////////////////////////////////         start:prod          /////////////////////////////////////": "",
    "start browser-sync server, serving the build directory reload if any files change in build directory": "",
    "start:prod": "browser-sync start --server 'build' --files 'build'",
    "3//////////////////////////////////////////         lint:js          ////////////////////////////////////////": "",
    "lint the .js files in src": "",
    "lint:js": "eslint './src/**/*.js'",
    "4//////////////////////////////////////////         lint:css          ///////////////////////////////////////": "",
    "lint the .css files in src": "",
    "lint:css": "stylelint './src/**/*.css'"
  },
  "dependencies": {
    "react": "^15.3.2",
    "react-dom": "^15.3.2",
    "sanitize.css": "^4.1.0"
  },
  "devDependencies": {
    "babel-cli": "6.18.0",
    "babel-core": "6.18.0",
    "babel-eslint": "^7.0.0",
    "babel-loader": "^6.2.5",
    "babel-preset-latest": "6.16.0",
    "babel-preset-react": "6.16.0",
    "babel-preset-stage-0": "6.16.0",
    "browser-sync": "^2.17.5",
    "css-loader": "^0.25.0",
    "eslint": "^3.9.1",
    "eslint-plugin-import": "^2.0.1",
    "eslint-plugin-jsx-a11y": "^2.2.3",
    "eslint-plugin-react": "^6.5.0",
    "extract-text-webpack-plugin": "2.0.0-beta.4",
    "file-loader": "^0.9.0",
    "html-webpack-plugin": "^2.24.1",
    "image-webpack-loader": "^3.0.0",
    "json-loader": "^0.5.4",
    "postcss-cssnext": "^2.8.0",
    "postcss-focus": "^1.0.0",
    "postcss-loader": "^1.0.0",
    "postcss-reporter": "^1.4.1",
    "rimraf": "^2.5.4",
    "style-loader": "0.13.1",
    "stylelint": "^7.5.0",
    "text-loader": "0.0.1",
    "url-loader": "^0.5.7",
    "webpack": "2.1.0-beta.25",
    "webpack-dev-server": "2.1.0-beta.9",
    "webpack-validator": "2.2.9"
  }
}
