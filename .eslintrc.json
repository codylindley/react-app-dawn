{
    "ecmaFeatures": {
        "jsx": true,
        "modules": true
    },
    "parser": "babel-eslint",
    "env": {
        "browser": true,
        "node": true
    },
    "extends": "./linters/js/index.js",
    "plugins": ["react"],
    "rules": {
        "react/jsx-uses-react": 2,
        "react/jsx-uses-vars": 2,
        "react/react-in-jsx-scope": 2
    }
}